P1 = input().split()
P2 = input().split()
player1 = [int(x) for x in P1]
player2 = [int(x) for x in P2]

s1=sum(player1)
s2=sum(player2)

len1=len(player1)
len2=len(player2)

avg1 = s1//len1
avg2 = s2//len2

if avg1 > avg2:
    print("Player 1 has better average than Player 2")
elif avg2 > avg1:
    print("Player 2 has better average than Player 1")
else:
    if s1 > s2:
        print("Player 1 scored more runs than Player 2")
    else:
        print("Player 2 scored more runs than Player 1")
